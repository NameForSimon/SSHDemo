<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.Book" table="book">
		<id name="bid" column="bid">
			<generator class="native"></generator>
		</id>
		<property name="bookname" column="bookname"></property>
		<property name="title" column="title"></property>
		<property name="news" column="news"></property>
		<property name="bookaddress" column="bookaddress"></property>
		<property name="bookphone" column="bookphone"></property>
		<property name="studentname" column="studentname"></property>
		<property name="about" column="about"></property>
		<property name="status" column="status"></property>
	</class>
</hibernate-mapping>